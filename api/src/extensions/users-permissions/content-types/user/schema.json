{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "firstname": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "lastname": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "gender": {
      "type": "string",
      "maxLength": 10
    },
    "address": {
      "type": "text",
      "maxLength": 200
    },
    "dob": {
      "type": "date"
    },
    "about": {
      "type": "richtext",
      "default": "Hi there",
      "required": false,
      "maxLength": 500
    },
    "linkedin_username": {
      "type": "string",
      "unique": true
    },
    "experiences": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::experience.experience",
      "mappedBy": "User"
    },
    "educations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::education.education",
      "mappedBy": "User"
    },
    "skills": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill.skill"
    },
    "github_url": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "instagram_url": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "portfolio_url": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "website_url": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "linkedin_url": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "twitter_url": {
      "type": "string",
      "maxLength": 100,
      "unique": true
    },
    "last_login": {
      "type": "date"
    }
  }
}
